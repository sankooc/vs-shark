{
	"name": "pcapviewer",
	"displayName": "pcapviewer",
	"description": "pcap/pcapng file analyzer",
	"version": "0.3.2",
	"publisher": "sankooc",
	"repository": {
		"type": "git",
		"url": "https://github.com/sankooc/vs-shark/tree/master/extension"
	},
	"engines": {
		"vscode": "^1.86.2"
	},
	"categories": [
		"Debuggers"
	],
	"icon": "icon.png",
	"keywords": [
		"pcap/pcapng",
		"network trace",
		"wireshark"
	],
	"license": "MIT",
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "proto.pcapng",
				"displayName": "pcapng",
				"selector": [
					{
						"filenamePattern": "*.pcapng"
					},
					{
						"filenamePattern": "*.pcap"
					}
				]
			}
		]
	},
	"scripts": {
		"build": "cd webview && npm run css && npm run build",
		"vscode:prepublish": "pnpm run package",
		"compile": "webpack && cp node_modules/rshark/nshark* dist/",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"vsce": {
	  "dependencies": false,
	  "yarn": true
	},
	"dependencies": {
    "lodash": "^4.17.21",
    "rshark": "file:../wasm/node"
  },
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "20.x",
		"@types/vscode": "^1.86.2",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.92.0",
		"webpack-cli": "^5.1.4"
	}
}