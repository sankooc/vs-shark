{
	"name": "pcapviewer",
	"displayName": "pcapviewer",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.86.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "Protocol",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "pcap.tree",
					"name": "protocol stack",
					"visibility": "visible",
					"contextualTitle": "protocol"
				},
				{
					"id": "pcap.detail",
					"name": "detail info",
					"visibility": "visible",
					"type": "webview",
					"contextualTitle": "binary title"
				}
			]
		},
		"commands": [
			{
				"command": "nodeDependencies.refreshEntry",
				"title": "Refresh"
			}
		],
		"customEditors": [
			{
				"viewType": "proto.pcapng",
				"displayName": "pcapng",
				"selector": [
					{
						"filenamePattern": "*.pcapng"
					},
					{
						"filenamePattern": "*.pcap"
					}
				]
			}
		]
	},
	"scripts": {
		"build": "sh build.sh",
		"vscode:prepublish": "pnpm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
		"lint": "eslint src --ext ts",
		"reset": "rm -rf node_modules/protocols && pnpm install",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "20.x",
		"@types/vscode": "^1.86.2",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.92.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"protocols": "file:../node-pcapng/"
	}
}
