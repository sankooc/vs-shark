{
	"name": "pcapviewer",
	"displayName": "pcapviewer",
	"description": "pcap/pcapng file analyzer",
	"version": "0.4.1",
	"publisher": "sankooc",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/sankooc/vs-shark/tree/master/plugin"
	},
	"homepage": "https://sankooc.github.io/vs-shark",
	"engines": {
		"vscode": "^1.86.2"
	},
	"categories": [
		"Debuggers"
	],
	"icon": "icon256.png",
	"keywords": [
		"pcap/pcapng",
		"network trace",
		"wireshark"
	],
	"license": "MIT",
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "proto.pcapng",
				"displayName": "pcapng",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.pcapng"
					},
					{
						"filenamePattern": "*.cap"
					},
					{
						"filenamePattern": "*.pcap"
					}
				]
			}
		]
	},
	"scripts": {
		"cp": "cp -r ../webview/src/share/ src/",
		"compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"format": "prettier -w ./src",
		"test": "vscode-test"
	},
	"vsce": {
		"dependencies": false,
		"yarn": true
	},
	"resolutions": {
      "cross-spawn": "^7.0.5",
	  "micromatch": "^4.0.8"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"rshark": "file:../crates/wasm/build/node"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "20.x",
		"@types/vscode": "^1.86.2",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
  		"esbuild": ">=0.25.0",
		"eslint": "^8.57.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"typescript": "5.4.5"
	}
}